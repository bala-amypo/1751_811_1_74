<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Demo Application Test Suite" verbose="1">

    <test name="Demo Project Tests">

        <!-- Run all groups used in your project -->
        <groups>
